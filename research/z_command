本地运行所用命令行： source activate tensorflow

******************* tinymind race001 
python download_and_convert_data.py --dataset_name=tmd001 --dataset_dir=./tmp/tmd001_raw_data
python image_augmentation.py --dataset_dir=./slim/tmp/tmd001_raw_data
python image_augmentation2.py --dataset_dir=./slim/tmp/tmd001_raw_data

****************** inception_v4
python train_image_classifier.py --dataset_name=tmd001 --dataset_dir=./tmp/tmd001 --checkpoint_path=./tmp/checkpoints/inception_v4.ckpt --model_name=inception_v4 --checkpoint_exclude_scopes=InceptionV4/Logits,InceptionV4/AuxLogits/Aux_logits --train_dir=./tmp/tmd001-models/inception_v4 --learning_rate=0.001 --optimizer=rmsprop  --batch_size=2

python eval_image_classifier.py --dataset_name=tmd001 --dataset_dir=./tmp/tmd001 --dataset_split_name=validation --model_name=inception_v4 --checkpoint_path=./tmp/tmd001-models/inception_v4 --eval_dir=./tmp/tmd001-models/inception_v4 --batch_size=8 --max_num_batches=12

python inference_with_checkpoint2.py --dataset_name=tmd001 --dataset_dir=./tmp/tmd001 --model_name=inception_v4 --checkpoint_path=./tmp/tmd001-models/inception_v4 --pic_path=./tmp/tmd001_raw_data/test2 --label_path=./tmp/tmd001/labels.txt

python inference_with_checkpoint2.py --dataset_name=tmd001 --dataset_dir=./tmp/tmd001 --model_name=inception_v4 --checkpoint_path=./tmp/tmd001-models/inception_v4/Exec5rmsprop --pic_path=./tmp/tmd001_raw_data/test1 --label_path=./tmp/tmd001/labels.txt

python inference_with_checkpoint3.py --dataset_name=tmd001 --dataset_dir=./tmp/tmd001 --model_name=inception_v4 --checkpoint_path=./tmp/tmd001-models/inception_v4/Exec4sgd --pic_path=./tmp/tmd001_raw_data/train/令 --label_path=./tmp/tmd001/labels.txt

python inference_with_checkpoint4.py --dataset_name=tmd001 --dataset_dir=./tmp/tmd001 --model_name=inception_v4 --checkpoint_path=./tmp/tmd001-models/inception_v4/Exec4sgd --pic_path=./tmp/tmd001_raw_data/train/ --label_path=./tmp/tmd001/labels.txt

#delete the wrong image
