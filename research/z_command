python eval_image_classifier.py --dataset_name=tmd001 --dataset_dir=./tmp/tmd001 --dataset_split_name=validation --model_name=nasnet_large --checkpoint_path=./tmp/tmd001-models/nasnet_large --eval_dir=./tmp/tmd001-models/nasnet_large --batch_size=2 --max_num_batches=12

